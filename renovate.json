{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "baseBranchPatterns": [
    "dep_updates"
  ],
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchDepTypes": ["dependencies", "devDependencies", "optionalDependencies"],
      "rangeStrategy": "pin"
    }
  ]
}
