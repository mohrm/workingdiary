{
  "name": "workingdiary",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e": "npx bddgen -c e2e/playwright.config.js && npx playwright test -c e2e/playwright.config.js",
    "prebuild": "node scripts/update-version.js",
    "build:ci": "ng build --configuration production",
    "e2e-prepare:ci": "npx playwright install --with-deps && npx bddgen -c e2e/playwright.config.js",
    "e2e:ci": "npx playwright test -c e2e/playwright.config.js",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.2.4",
    "@angular/cdk": "20.2.5",
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/material": "20.2.5",
    "@angular/platform-browser": "20.2.4",
    "@angular/platform-browser-dynamic": "20.2.4",
    "@angular/router": "20.2.4",
    "@angular/service-worker": "20.2.4",
    "moment": "2.30.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.3.1",
    "@angular/cli": "20.3.1",
    "@angular/compiler-cli": "20.2.4",
    "@cucumber/cucumber": "12.2.0",
    "@playwright/test": "1.55.1",
    "@types/jasmine": "5.1.9",
    "angular-eslint": "20.2.0",
    "eslint": "9.36.0",
    "jasmine-core": "5.10.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "npm-run-all2": "8.0.4",
    "playwright-bdd": "8.4.1",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0"
  }
}
