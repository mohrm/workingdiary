{
  "name": "workingdiary",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "npx bddgen -c e2e/playwright.config.js && npx playwright test -c e2e/playwright.config.js",
    "prebuild": "node scripts/update-version.js",
    "build:ci": "ng build --configuration production",
    "e2e-prepare:ci": "npx playwright install --with-deps && npx bddgen -c e2e/playwright.config.js",
    "e2e:ci": "npx playwright test -c e2e/playwright.config.js",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "21.1.1",
    "@angular/cdk": "21.1.1",
    "@angular/common": "21.1.1",
    "@angular/compiler": "21.1.1",
    "@angular/core": "21.1.1",
    "@angular/forms": "21.1.1",
    "@angular/material": "21.1.1",
    "@angular/platform-browser": "21.1.1",
    "@angular/platform-browser-dynamic": "21.1.1",
    "@angular/router": "21.1.1",
    "@angular/service-worker": "21.1.1",
    "moment": "2.30.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.16.0"
  },
  "devDependencies": {
    "@angular/build": "21.1.1",
    "@angular/cli": "21.1.1",
    "@angular/compiler-cli": "21.1.1",
    "@cucumber/cucumber": "12.5.0",
    "@playwright/test": "1.57.0",
    "@types/jest": "^30.0.0",
    "angular-eslint": "21.1.0",
    "eslint": "9.39.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^16.0.0",
    "karma-coverage": "2.2.1",
    "npm-run-all2": "8.0.4",
    "playwright-bdd": "8.4.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1"
  }
}
